  "scripts": {
    "dev": "NODE_ENV=development nodemon api/index.js --exec babel-node --presets @babel/preset-env --watch api --watch package.json",
    "build": "yarn build:next && yarn build:server",
    "build:next": "next build",
    "build:server": "NODE_ENV=production babel api/index.js --out-dir .next/dist",
    "start": "NODE_ENV=production node .next/dist/index.js",
    "lint": "eslint --color ./ *.js"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel",
          [
            "@babel/env",
            {
              "targets": {
                "node": "8"
              }
            }
          ]
        ]
      }
    }
  },
